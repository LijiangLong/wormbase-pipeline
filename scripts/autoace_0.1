#!/usr/bin/csh 

setenv ACEDB_NO_BANNER
setenv ACEDB /wormsrv2/autoace

switch (`uname`)
  case "IRIX":
        rsh wormsrv2 "~wormpub/acedb/RELEASE.DEVELOPMENT/bin.SGI_4/xace $ACEDB"
        breaksw
  case "OSF1":
        rsh wormsrv2 "~wormpub/acedb/RELEASE.DEVELOPMENT/bin.ALPHA_4/xace $ACEDB"
        breaksw
  case "Linux":
        rsh wormsrv2 "~wormpub/acedb/RELEASE.DEVELOPMENT/bin.LINUX/xace $ACEDB"
        breaksw
  case "SunOS":
        switch (`ostype`)
          case "SunOS":
                rsh wormsrv2 "~wormpub/acedb/RELEASE.DEVELOPMENT/bin.SUN_4/xace $ACEDB"
                breaksw
          case "Solaris":
                rsh wormsrv2 "~wormpub/acedb/RELEASE.DEVELOPMENT/bin.SOLARIS_4/xace $ACEDB"
                breaksw
        endsw
        breaksw
endsw

