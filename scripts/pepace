#!/bin/csh -f

setenv ACEDB_NO_BANNER
setenv ACEDB /wormsrv2/pepace

switch (`uname`)
  case "IRIX":
	~wormpub/acedb/ace4/bin.SGI_4/xace $ACEDB
	breaksw
  case "OSF1":
	~acedb/RELEASE.DEVELOPMENT/bin.ALPHA_4/xace $ACEDB
	breaksw
  case "SunOS":
	switch (`ostype`)
	  case "SunOS":
		~wormpub/acedb/ace4/bin.SUN_4/xace $ACEDB
		breaksw
	  case "Solaris":
#		~wormpub/acedb/ace4/bin.SUN_4/xace $ACEDB
		~wormpub/acedb/ace4/bin.SOLARIS_4/xace $ACEDB
		breaksw
	endsw
	breaksw
endsw
