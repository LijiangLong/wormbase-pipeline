// new slimmed down Locus class.  Will initially just store Polymorphisms and gene clusters.
// krb Feb 2004
 
?Locus  Evidence #Evidence
        Name    Other_name ?Gene_name #Evidence // unidirectional link
        Species UNIQUE ?Species // added by krb 15/02/02
        Type    Gene_cluster    Contains ?Gene XREF In_cluster
                Polymorphism    SNP  Text               // SNP stuff added for Todd [171001 krb]
                                Status Text
                                SNP_assay ?PCR_product XREF SNP_locus ?Text
                                RFLP    Text
                                Transposon_insertion  Text
                                Detection_method Text
        Map    UNIQUE ?Map XREF Locus #Map_position
        Positive Inside_rearr ?Rearrangement XREF Locus_inside ?Author
                 Positive_clone ?Clone XREF Positive_locus #Evidence
        Negative Outside_rearr ?Rearrangement XREF Locus_outside ?Author
                 Negative_clone ?Clone XREF Negative_locus ?Author
        Mapping_data    Well_ordered            // "on the line"
                        2_point ?2_point_data
                        Multi_point ?Multi_pt_data
                        Pos_neg_data ?Pos_neg_data
        Allele  ?Allele XREF Locus #Evidence // just for SNPs for now
        Laboratory ?Laboratory
        Reference ?Paper XREF Locus
        Remark ?Text #Evidence
 


////////////////////////////////////////////////////////////////////////////////////////
//
//  The long-overdue, much awaited ?Gene class, to replace ?Locus
//
////////////////////////////////////////////////////////////////////////////////////////
                                                                                
                                                                                
?Gene   Evidence #Evidence
        Identity  Version UNIQUE Int
                  Name  CGC_name        UNIQUE ?Gene_name XREF CGC_name_for
                        Sequence_name   UNIQUE ?Gene_name XREF Sequence_name_for // captures main CDS name without isoforms
			Molecular_name         ?Gene_name XREF Molecular_name_for  // for linking to full CDS names, wormpep etc.
                        Other_name             ?Gene_name XREF Other_name_for #Evidence // other unapproved CGC-style gene names
                        Public_name     UNIQUE ?Gene_name XREF Public_name_for // the one name that should be used
                  Species UNIQUE ?Species
                  History Version_change Int UNIQUE DateType UNIQUE ?Person #Gene_history_action
                          Merged_into UNIQUE ?Gene XREF Acquires_merge
                          Acquires_merge ?Gene XREF Merged_into
                          Split_from UNIQUE ?Gene XREF Split_into
                          Split_into ?Gene XREF Split_from
                  Live  // remove live to make gene "dead"
        Gene_info Gene_class UNIQUE ?Gene_class XREF Genes
                  Laboratory ?Laboratory
                  Reference_allele ?Allele Text
                  Allele ?Allele XREF Gene #Evidence
                  Phenotype ?Text #Evidence
                  Provisional_description ?Text #Evidence // added for Erich [020322 krb]
                  Detailed_description ?Text #Evidence // added for Erich [020322 krb]
                  Concise_description ?Text #Evidence // added for Erich [020523 krb]
                  Complementation_data Text
                  Strain ?Strain XREF Gene
                  In_cluster ?Locus XREF Contains // new way of linking to Clusters in locus class
                  GO_term ?GO_term XREF Gene ?GO_code #Evidence
        Molecular_info CDS ?CDS XREF Gene #Evidence             // to link to live CDSs
                       CDS_history ?CDS XREF Gene_history       // to link to history CDSs
                       Transcript ?Transcript XREF Gene #Evidence
                       Transcript_history ?Transcript XREF Gene_history
                       Pseudogene ?Pseudogene XREF Gene #Evidence
                       Pseudogene_history ?Pseudogene XREF Gene_history
                       Gene_variant_count UNIQUE Int    // how many isoforms have been made (including Pseudogenes and non coding)
                                                        // intended for acedb code to look at this when suggesting new gene name
                       Other_sequence ?Sequence XREF Gene #Evidence  // for linking to mRNAs etc. that correspond to a gene		   
        Experimental_info RNAi_result ?RNAi XREF Gene
                          Expr_pattern ?Expr_pattern XREF Gene
                          Drives_Transgene ?Transgene XREF Driven_by_gene
                          Transgene_product ?Transgene XREF Gene
                          Rescued_by_transgene ?Transgene
                          Gene_regulation Trans_regulator ?Gene_regulation XREF Trans_regulator_gene
                                          Trans_target    ?Gene_regulation XREF Trans_regulated_gene
                          Antibody ?Antibody XREF Gene
                          Microarray_results ?Microarray_results XREF Gene
			  SAGE_transcript ?SAGE_transcript XREF Gene
			  Y2H_bait   ?Y2H XREF Bait_overlapping_gene // for yeast two hybrid data
			  Y2H_target ?Y2H XREF Target_overlapping_gene
			  3d_data ?3d_data XREF Gene // for NESGC structure information
        Map_info  Map UNIQUE ?Map XREF Gene #Map_position
                  Well_ordered          // "on the line"
                  Hide_under ?Gene XREF Representative_for
                  Representative_for ?Gene XREF Hide_under
                  Positive Inside_rearr   ?Rearrangement XREF Gene_inside #Evidence
                           Positive_clone ?Clone XREF Positive_gene #Evidence
                  Negative Outside_rearr  ?Rearrangement XREF Gene_outside #Evidence
                           Negative_clone ?Clone XREF Negative_gene #Evidence
                  Mapping_data  2_point ?2_point_data
                                Multi_point ?Multi_pt_data
				Pos_neg_data ?Pos_neg_data
                  Interpolated_map_position UNIQUE ?Map UNIQUE Float // for loci with CDSs but no real Map position
        Reference ?Paper XREF Gene
        Remark ?Text #Evidence


